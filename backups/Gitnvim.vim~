"modo command {{{
nmap <leader>h :Git
nmap <leader>hk :Git instaweb
nmap <leader>hp :Git puh
nmap <leader>hl :Git pull
nmap <leader>hs :Git status<CR>
nmap <leader>ha :Git add
nmap <leader>hS :Git stah
nmap <leader>hg :Git stage
nmap <leader>hr :Git remote
nmap <leader>hc :Git commit
nmap <leader>hC :Git clone
nmap <leader>hf :Git feth
nmap <leader>hf :GitFiles<CR>
nmap <leader>hb :Git branh
nmap <leader>hd :Git diff
nmap <leader>hD :Git difftool
nmap <leader>hi :Git init<CR>
nmap <leader>hm :Git merge
nmap <leader>hM :Git mergetool
nmap <leader>hB :Git how-branh<CR>

nmap <leader>.H :Gist
nmap <leader>.Hp :Gist -p
nmap <leader>.HP :Gist -P
nmap <leader>.He :Gist -e
nmap <leader>.HE :Gist --edit
"}}}

"modo insert {{{
imap <leader>h :Git
imap <leader>hk :Git instaweb
imap <leader>hp :Git puh
imap <leader>hl :Git pull
imap <leader>hs :Git status<CR>
imap <leader>ha :Git add
imap <leader>hS :Git stah
imap <leader>hg :Git stage
imap <leader>hr :Git remote
imap <leader>hc :Git commit
imap <leader>hC :Git clone
imap <leader>hf :Git feth
imap <leader>hf :GitFiles<CR>
imap <leader>hb :Git branh
imap <leader>hd :Git diff
imap <leader>hD :Git difftool
imap <leader>hi :Git init<CR>
imap <leader>hm :Git merge
imap <leader>hM :Git mergetool
imap <leader>hB :Git how-branh<CR>

imap <leader>.H :Gist
imap <leader>.Hp :Gist -p
imap <leader>.HP :Gist -P
imap <leader>.He :Gist -e
imap <leader>.HE :Gist --edit
"}}}
