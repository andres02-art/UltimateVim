"modo normal {{{
nnoremap <leader>fd <cmd>Telescope find_files<cr>
nnoremap <leader>fw <cmd>Telescope grep_string<cr>
nnoremap <leader>fg <cmd>Telescope live_grep<Cr>
nnoremap <leader>.f <cmd>Telescope filetypes<cr>
nnoremap <leader>fb <cmd>Telescope buffers<cr>
nnoremap <leader>fh :Telescope git_

nnoremap <leader>FF <cmd>Grepper -highlight -tool rg<CR>
nnoremap <leader>Ff <cmd>GrepperRg<CR>
nnoremap <leader>FH <cmd>Grepper -highlight -tool git<CR>
nnoremap <leader>Fh <cmd>GrepperGit<CR>
nnoremap <leader>FS <cmd>Grepper -highlight -tool findstr<CR>
nnoremap <leader>Fs <cmd>GrepperFindstr<CR>
"}}}

"modo normal {{{
inoremap <leader>fd <cmd>Telescope find_files<cr>
inoremap <leader>fw <cmd>Telescope grep_string<cr>
inoremap <leader>fg <cmd>Telescope live_grep<Cr>
inoremap <leader>.f <cmd>Telescope filetypes<cr>
inoremap <leader>fb <cmd>Telescope buffers<cr>
inoremap <leader>fh <leader>nn:Telescope git_

inoremap <leader>FF <cmd>Grepper -highlight -tool rg<CR>
inoremap <leader>Ff <cmd>GrepperRg<CR>
inoremap <leader>FH <cmd>Grepper -highlight -tool git<CR>
inoremap <leader>Fh <cmd>GrepperGit<CR>
inoremap <leader>FS <cmd>Grepper -highlight -tool findstr<CR>
inoremap <leader>Fs <cmd>GrepperFindstr<CR>
"}}}
