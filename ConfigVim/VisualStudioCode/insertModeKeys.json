   {
        "before": [
            "<leader>",
            "k"
        ],
        "commands": [
            "workbench.action.files.save"
        ]
    },
    {
        "before": ["<leader>","q"],
        "commands": ["workbench.action.closeActiveEditor"]
    },
    {
        "before" : ["<leader>","p"],
        "after":["<Esc>","p","a"]
    },
    {
        "before" : ["<leader>","P"], 
        "after" : ["<Esc>","P","a"]
    },   
    {
        "before": ["<C-s>"],
        "after" : ["<leader>","k","<leader>","q"]
    },
    {
        "before": ["<C-j>"], 
        "after": ["<Esc>","d","d","0","p","i"] 
    }, 
    {
        "before": ["<leader>","<leader>"],
        "commands": ["workbench.action.terminal.splitInstance"]
    }
